		"@types/cookie": "^0.5.1",
		"gh-pages": "^6.1.1",
		"mdsvex": "^0.11.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.1.4"
	},
	"type": "module",
	"dependencies": {
		"svelte-device-info": "^1.0.4"
